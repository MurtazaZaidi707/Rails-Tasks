<h1>Hello, Rails!</h1>
<h1>Posts#index</h1>
<p>Find me in app/views/posts/index.html.erb</p>

<h1>Posts</h1>

<ul>
  <% @posts.each do |postss| %>
    <li>
      <%= postss.title %>
    </li>
  <% end %>
</ul>

<ul>
  <% @posts.each do |article| %>
    <li>
      <a href="/posts/<%= article.id %>">
        <%= article.title %>
      </a>
    </li>
  <% end %>
</ul>
